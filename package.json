    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "shipit-shared": "^4.4.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.13.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  }
}
